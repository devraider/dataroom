# Application Mode
# Options: development, production
MODE=development

# Demo Mode - Set to true to use mock data instead of backend
VITE_DEMO_MODE=true

# Backend API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_API_TIMEOUT=30000

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_API_KEY=your_google_api_key_here
VITE_GOOGLE_SCOPE=https://www.googleapis.com/auth/drive.readonly
VITE_GOOGLE_DISCOVERY_DOCS=["https://www.googleapis.com/discovery/v1/apis/drive/v3/rest"]
