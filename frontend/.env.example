# Application Mode
# Options: demo, development, production
# `demo` mode uses mock data and does not require a backend server.
MODE=development


# Backend API Configuration
VITE_API_URL=http://localhost:5000/api
VITE_API_TIMEOUT=30000

# Google OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_SCOPE=https://www.googleapis.com/auth/drive.readonly
VITE_GOOGLE_DISCOVERY_DOCS=["https://www.googleapis.com/discovery/v1/apis/drive/v3/rest"]
